{
    "manifest_version": 2,
    "name": "查看我的IP",
    "version": "1.0",
    "description": "查看我的电脑当前的公网IP",
    "browser_action": {
        "default_icon": {
            "19": "images/icon.png",
            "38": "images/icon.png"
        },
        "default_title": "查看我的IP",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "js/popup.js"
        ]
    },
    "options_page": "options.html",
    "web_accessible_resources": [
      "images/icon.png"
    ],
    "permissions": [
        "notifications"
    ]
}